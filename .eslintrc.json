{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "next",
        "wesbos/typescript"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["postcss.config.js", "tailwind.config.js", "next.config.mjs"]
}
